# Database
DB_HOST=127.0.0.1
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name

# JWT
JWT_SECRET=your_jwt_secret

# Stripe (optional)
# If you set STRIPE_SECRET_KEY the server will use Stripe. To force mock mode set STRIPE_MOCK=true
#STRIPE_SECRET_KEY=sk_test_...
STRIPE_MOCK=true

# Frontend allowed origin (optional). If set, CORS will be restricted to this origin.
#FRONTEND_URL=http://localhost:8080

# Server port
PORT=3000
