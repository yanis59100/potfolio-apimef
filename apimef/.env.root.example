# Apimef - Configuration Locale
# Copier ce fichier vers .env.local et adapter les valeurs

# Mode développement avec mock Stripe (pas besoin de clés Stripe)
STRIPE_MOCK=true

# Base de données
DB_HOST=localhost
DB_USER=apimef
DB_PASSWORD=your_password_here
DB_NAME=apimef

# JWT pour authentification
JWT_SECRET=your_super_secret_jwt_key_here

# API Backend (pour frontend)
VUE_APP_API_BASE_URL=http://localhost:3000

# CORS (optionnel, laisser vide pour permettre tous les origins en dev)
FRONTEND_URL=http://localhost:8080

# Port du serveur backend
PORT=3000
